<%- include('header') %>
<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>

<div class="box">
  <% tasks.forEach(task => { %>
  <div class="item">
    <input type="checkbox" />
    <p><%= task.name %></p>
  </div>
  <% }) %>

  <form class="item" action="/" method="post">
    <input name="task" type="text" placeholder="Enter Task" />
    <button type="submit" name="list" value=<%=listTitle %>>+</button>
  </form>
</div>

<%- include('footer') %>
